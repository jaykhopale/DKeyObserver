	<GlossaryItems>
		<GlossaryItem>
			<itemID>1</itemID>
			<title>Perennial</title>
			<itemImage>NoImage.png</itemImage>
			<description>lives for more than one year.</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>2</itemID>
			<title>Annual</title>
			<itemImage>NoImage.png</itemImage>
			<description>completes its life cycle in one season or year.</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>3</itemID>
			<title>Calyx</title>
			<itemImage>Calyx.png</itemImage>
			<description>collective term for all sepals</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>4</itemID>
			<title>Cauline leaf</title>
			<itemImage>Cauline.png</itemImage>
			<description>attached to a stem above its base</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>5</itemID>
			<title>Basal leaf</title>
			<itemImage>BasalLeaf.png</itemImage>
			<description>attached to base of stem</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>6</itemID>
			<title>Pinnately compound leaf</title>
			<itemImage>CompoundLeaf.png</itemImage>
			<description>leaflets arranged along the main axis</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>7</itemID>
			<title>Leaflet</title>
			<itemImage>CompoundLeaf.png</itemImage>
			<description>part of a compound leaf.</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>8</itemID>
			<title>Simple leaf</title>
			<itemImage>SimpleLeaf.png</itemImage>
			<description>undivided leaf, if it has lobes, they do not reach the
				main vein.</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>9</itemID>
			<title>Narrow calyx membrane</title>
			<itemImage>NarrowCalyx.jpg</itemImage>
			<description>see illustration.</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>10</itemID>
			<title>Wide calyx membrane</title>
			<itemImage>WideCalyx.jpg</itemImage>
			<description>see illustration.</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>11</itemID>
			<title>opposite leaves</title>
			<itemImage>OppositeLeaves</itemImage>
			<description>two leaves at each node</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>12</itemID>
			<title>alternate leaves</title>
			<itemImage>AlternateLeaves.png</itemImage>
			<description>one leaf at each node</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>13</itemID>
			<title>palmately lobed leaf</title>
			<itemImage>PalmateLobe.png</itemImage>
			<description>having three or more lobes whose axes radiate from a
				common point</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>14</itemID>
			<title>terminal leaflet</title>
			<itemImage>CompoundLeaf.png</itemImage>
			<description>leaflet at the end of the axis of a compound leaf</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>15</itemID>
			<title>lateral leaflet</title>
			<itemImage>CompoundLeaf.png</itemImage>
			<description>leaflet attached along the axis of a compound leaf</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>16</itemID>
			<title>bract</title>
			<itemImage>Calyx.png</itemImage>
			<description>modified leaf associated with a flower or inflorescence</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>17</itemID>
			<title>stamen</title>
			<itemImage>Flower.png</itemImage>
			<description>pollen producing (male) part of a flower</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>18</itemID>
			<title>corolla</title>
			<itemImage>Flower.png</itemImage>
			<description>collective term for all petals</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>19</itemID>
			<title>inflorescence</title>
			<itemImage>Navarretia.png</itemImage>
			<description>collection of flowers arranged on a stem consisting of
				one or more branches</description>
		</GlossaryItem>
		<GlossaryItem>
			<itemID>20</itemID>
			<title>Petals</title>
			<itemImage>Flower.png</itemImage>
			<description>modified leaves surrounding the reproductive parts of a
				flower.</description>
		</GlossaryItem>
	</GlossaryItems>
</Key_components>